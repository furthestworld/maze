{
    "name": "edoger/maze",
    "description": "Maze is a PHP configuration file manager.",
    "keywords": ["configuration", "maze", "edoger"],
    "license": "Apache-2.0",
    "type": "library",
    "homepage": "https://github.com/edoger/maze",
    "authors": [
        {
            "name": "Qingshan Luo",
            "email": "shanshan.lqs@gmail.com",
            "homepage": "https://www.edoger.com",
            "role": "developer"
        }
    ],
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.7"
    },
    "autoload": {
        "psr-4": {
            "Maze\\": "Maze/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MazeTest\\": "Tests/MazeTest/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
